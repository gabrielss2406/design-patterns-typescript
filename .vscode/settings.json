{
    "code-runner.executorMap": {
        "typescript": "npx ts-node --files --transpile-only"
    },
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "always",
      "source.fixAll": "always"
    },
    "eslint.enable": true,
    "eslint.run": "onType",
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ]
}